<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background">
        <shape>
            <solid
                android:color="#00000000" />
        </shape>
    </item>
    <item
        android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:startColor="#ffc5eaf8"
                    android:endColor="#ff2ab0e2"
                    android:angle="270.0"
                    android:centerY="0.75"
                    android:centerColor="#ff2ab0e2" />
            </shape>
        </clip>
    </item>
    <item
        android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:startColor="#ffc5eaf8"
                    android:endColor="#ff2ab0e2"
                    android:angle="270.0"
                    android:centerY="0.75"
                    android:centerColor="#ff2ab0e2" />
            </shape>
        </clip>
    </item>
</layer-list>