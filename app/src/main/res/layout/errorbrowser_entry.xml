<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    android:padding="2.0dip"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:id="@id/errorbrowserEntryFile"
        android:paddingLeft="20.0dip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@id/errorbrowserEntryFileImage"
            style="@style/BrowserImage" />
        <TextView
            android:id="@id/errorbrowserEntryFileName"
            android:paddingLeft="3.0dip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            style="@style/BrowserLabelLarge" />
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:id="@id/errorbrowserEntryError"
        android:paddingLeft="40.0dip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:id="@id/errorbrowserEntryImage"
                style="@style/BrowserImage" />
            <TextView
                android:id="@id/errorbrowserEntryMessage"
                android:paddingLeft="3.0dip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=" "
                style="@style/BrowserLabelSmall" />
        </LinearLayout>
    </LinearLayout>
</FrameLayout>